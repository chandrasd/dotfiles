{
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet"
    }
  },
  "icon_theme": "Monospace Icon Theme - Light",
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
      "enterprise_uri": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "base_keymap": "VSCode",
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light Hard",
    "dark": "Gruvbox Dark"
  },
  "ui_font_family": "Zed Plex Mono",
  "ui_font_size": 18,
  "buffer_font_size": 18,
  "agent_font_size": 18,
  "tab_size": 4,
  "terminal": {
    "font_family": "Zed Plex Mono",
    "font_size": 18,
    "blinking": "off",
    "cursor_shape": "bar",
    "keep_selection_on_copy": true
  },
  "confirm_quit": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "diagnostics": {
    "inline": {
      "enabled": true,
      "max_severity": null // same values as the `diagnostics_max_severity` from the editor settings
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "experimental.theme_overrides": {
    "background.appearance": "blurred",
    "title_bar.background": "#00000099"
  },
  "lsp": {
    "biome": {
      "settings": {
        "require_config_file": true
      }
    }
  }
}
